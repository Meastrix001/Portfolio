{
  "name": "my-portfolio",
  "version": "1.0.0",
  "description": "A simple landing page made with Eleventy",
  "main": "index.js",
  "scripts": {
    "eleventy": "npx eleventy",
    "sass": "sass --style=compressed src/style/main.scss src/main.css",
    "watch:sass": "npm run sass -- --watch",
    "watch:eleventy": "npx eleventy --serve",
    "start": "npm-run-all sass --parallel watch:*",
    "build": "npm run eleventy && npm run sass" 
    
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Nick Roofthooft",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "sass": "^1.32.8"
  }
}