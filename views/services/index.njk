  </section >

  <section class="services--container--act--content container">

{% for item in acts %}
  <div>
    <h3>{{ item.title }}</h3>
    <p class="text-gray">{{ item.desc }}</p>
  </div>
  <span></span>
{% endfor %}

  </section>
</div>

<div class="sectionDevider spacing">
