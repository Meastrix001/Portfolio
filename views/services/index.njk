<div class="services--cards spacing">
  <div class="services--cards--container container"> 
    {% for item in services %}
    <article class="services--cards--container--card ">
      <p><span class="text-red">{{ item.title }}</span></p>
      <h2>{{ item.subTitle }}</h2>
      <p class="text-gray">{{ item.desc }}</p>
      <section>
        </section>
    </article>
    {% endfor %}
    </div>
</div>

<div class="sectionDevider spacing">
