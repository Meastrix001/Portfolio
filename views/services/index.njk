<div class="services--container spacing">
        <div id="skills" class="skills">
    <section class="container spacing" >
    <h2>Languages and tools i currently work with</h2>
    <div class="titleUnderlined"></div>
    <section class="skills--skills">
        <div class="skills--list">
        {% for skill in skills %}
            {% if skill.learning === 0 %}         
                <div>
                    <img src={{ root }}{{ skill.logo }}>
                    <p>{{ skill.skill }}</p>
                </div>
            {% endif %}
        {% endfor %}
        </div>
    </section>
      <h2>Languages and tools i am currently learning</h2>
      <div class="titleUnderlined"></div>
          <section class="skills--skills">
        <div class="skills--list">
        {% for skill in skills %}
            {% if skill.learning === 1 %}         
                <div>
                    <img src={{ root }}{{ skill.logo }}>
                    <p>{{ skill.skill }}</p>
                </div>
            {% endif %}
        {% endfor %}
        </div>
    </section>
    
    </section>
</div>

